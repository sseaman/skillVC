{
    "_comment" : "Configuration for generating documentation for SkillVC",
    "_comment" : "To build: jsdoc -c ./docConfig.json ",
     
    "applicationName": "SkillVC",
    "disqus": "",
    "googleAnalytics": "UA-84944684-1",
    "title": "SkillVC",
    "openGraph": {
        "title": "SkillVC",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./README.md",
            "./lib/"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs"
      
    },
    "templates" : {
        "default" : {
            "layoutFile" : "./docLayout.tmpl"
        }
    }
}

